C900 = "Usage: /p1 [object... [/p3|{/p4 chg...}]] [/p5 workspace_name] [/p6 ws_tree_name...] [/p12 dest_workspace_name] [/p13] [/p14] [/p15 [/p16 char]] [/p17 file_path] [/p2] (1)
